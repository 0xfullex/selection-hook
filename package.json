{
  "name": "selection-hook",
  "version": "0.9.9",
  "description": "Text selection monitoring of native Node.js module with N-API across applications",
  "author": "0xfullex",
  "repository": {
    "type": "git",
    "url": "https://github.com/0xfullex/selection-hook"
  },
  "main": "index.js",
